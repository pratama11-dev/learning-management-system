                      data-settings="{&quot;background_background&quot;:&quot;classic&quot;}"
                      style="width: auto; height: 160px !important;"
                    ]
                      - if lesson.banner_image.present?
                        .elementor-column-wrap.elementor-element-populated style="background-image: url(#{url_for(lesson.banner_image)}) !important;"
                          .elementor-background-overlay
                          .elementor-widget-wrap
                            .elementor-element.elementor-element-f3940c0.elementor-widget.elementor-widget-image[
