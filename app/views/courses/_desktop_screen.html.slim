                                    .d-flex.flex-wrap.align-items-start
                                      .col-lg-6.pl-0
                                        .d-flex.flex-column.flex-wrap
                                          span.text-break.mb-3 [style="line-height: normal !important;"] = lesson.description.truncate(50)
                                          span.text-white.m-0 [style="line-height: normal !important;"] category
                                      .col-lg-6.px-0
